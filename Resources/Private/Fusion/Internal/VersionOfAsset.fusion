prototype(Carbon.IncludeAssets:Internal.VersionOfAsset) < prototype(Neos.Fusion:Component) {
    @styleguide {
        description = 'This prototype used as a cache buster'
    }

    path = null
    isFirst = true

    renderer = Neos.Fusion:Value {
        divider = ${props.isFirst ? '?' : '&'}
        hash = ${Carbon.FileContent.pathHash(props.path)}
        value = ${String.isBlank(this.hash) ? '' : this.divider + 'h=' + this.hash}
    }
}
