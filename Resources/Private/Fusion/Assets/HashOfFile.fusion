prototype(Carbon.IncludeAssets:HashOfFile) < prototype(Neos.Fusion:Value) {
	path = null
	resource = null

	@context {
		path = ${this.path}
		resource = ${this.resource}
	}

	fileContent = Carbon.IncludeAssets:FileContent {
		path = ${path}
		resource = ${resource}
	}

	value = ${(this.path || this.resource) && this.fileContent ? String.md5(this.fileContent) : ''}
}
