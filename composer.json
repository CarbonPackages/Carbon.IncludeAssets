{
    "name": "carbon/includeassets",
    "type": "neos-plugin",
    "description": "Include your assets (css, js) in an easy way into Neos",
    "license": "MIT",
    "require": {
        "neos/neos": "~3.0 || ~4.0",
        "carbon/filecontent": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Carbon\\IncludeAssets\\": "Classes"
        }
    },
    "archive": {
        "exclude": [".editorconfig", ".gitignore", ".github", ".gitattributes", "gulp.json", "package.json", "CODE_OF_CONDUCT.md"]
    },
    "authors": [
        {
            "name": "Jon Uhlmann",
            "email": "me@jonnitto.ch",
            "homepage": "https://github.com/jonnitto",
            "role": "Developer"
        }
    ],
    "extra": {
        "marketplace-account": "me@jonnitto.ch",
        "installer-name": "Carbon.IncludeAssets",
        "neos": {
            "package-key": "Carbon.IncludeAssets"
        }
    }
}
